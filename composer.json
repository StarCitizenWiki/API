{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.2",
        "ext-dom": "*",
        "ext-json": "*",
        "ext-libxml": "*",
        "dingo/api": "2.0.0-alpha2",
        "fabpot/goutte": "^3.2",
        "fideloper/proxy": "~4.0",
        "guzzlehttp/guzzle": "^6.3",
        "laravel/framework": "5.7.*",
        "laravel/socialite": "^3.0",
        "mediawiki/oauthclient": "^0.1.2",
        "piwik/piwik-php-tracker": "^1.2",
        "socialiteproviders/manager": "^3.3",
        "starcitizenwiki/deeply": "dev-master",
        "starcitizenwiki/mediawikiapi": "dev-develop",
        "starcitizenwiki/socialite-provider": "^1.0",
        "vinkla/hashids": "^5.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.3",
        "caouecs/laravel-lang": "^3.0",
        "doctrine/dbal": "^2.5",
        "escapestudios/symfony2-coding-standard": "~2.0",
        "fzaninotto/faker": "~1.4",
        "filp/whoops": "~2.0",
        "laravel/tinker": "~1.0",
        "mockery/mockery": "~1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0",
        "squizlabs/php_codesniffer": "2.*",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "repositories": [
        {
            "type": "vcs",
            "url":  "https://git.octofox.de/StarCitizenWiki/laravel-mediawiki-api.git"
        },
        {
            "type": "vcs",
            "url":  "https://git.octofox.de/StarCitizenWiki/SocialiteProvider.git"
        },
        {
            "type": "vcs",
            "url":  "https://git.octofox.de/StarCitizenWiki/deeply.git"
        }
    ],
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
